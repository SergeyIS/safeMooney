<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.safemooney">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_ico"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_ico"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".app.activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".app.activities.MainActivity"></activity>
        <activity android:name=".app.activities.AddTransactionActivity"></activity>
        <activity android:name=".app.activities.FindUserActivity"></activity>
    </application>

</manifest>